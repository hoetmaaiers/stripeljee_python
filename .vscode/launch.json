  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "stripeljee_python.main:app",
        "--reload",
        "--port",
        "8020",
      ],
      "jinja": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/stripeljee_python"
      },
      "cwd": "${workspaceFolder}/stripeljee_python"
    },
  ]
}